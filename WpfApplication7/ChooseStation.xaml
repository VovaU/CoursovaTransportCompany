<Window x:Class="WpfApplication7.ChooseStation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication7"
        mc:Ignorable="d"
         WindowStartupLocation="CenterScreen"
        Title="ChooseStation" Height="171.4" Width="308.15">
    <Grid Margin="0,0,-6.4,0.6">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="160,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="68"/>
        <Button x:Name="button" Content="Accept" HorizontalAlignment="Left" Margin="125,105,0,6" Width="75" Click="button_Click"/>
        <Label x:Name="label" Content="Station Number" HorizontalAlignment="Left" Margin="0,42,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
